{
  "name": "yeoman-generator-runner",
  "version": "1.0.0",
  "description": "Yeoman generator runner",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "format": "prettier --check **/*.ts",
    "format-fix": "prettier --write **/*.ts",
    "package": "ncc build --source-map",
    "act-run-job": "act -P ubuntu-latest=nektos/act-environments-ubuntu:18.04 -W ./__tests__/workflows/ -s GITHUB_TOKEN=$GITHUB_TOKEN -j",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "engines": {
    "node": ">=12.22.0",
    "npm": ">=7.24.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achieve-3000/yeoman-generator-runner.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Achieve 3000. <developers@achieve3000.com>",
  "license": "MIT",
  "homepage": "https://github.com/achieve-3000/yeoman-generator-runner#readme",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "@actions/glob": "^0.2.0",
    "yeoman-environment": "^2.0.0"
  },
  "devDependencies": {
    "@octokit/plugin-rest-endpoint-methods": "^5.5.1",
    "@types/inquirer": "^7.3.2",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^16.11.9",
    "@types/yeoman-environment": "^2.10.5",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.0.5",
    "husky": "^6.0.0",
    "jest": "^27.2.5",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.4"
  }
}
