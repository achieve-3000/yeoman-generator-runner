{
  "name": "yeoman-generator-runner",
  "version": "1.0.0",
  "description": "Yeoman generator runner",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "format": "prettier --check **/*.ts",
    "format-fix": "prettier --write **/*.ts",
    "package": "ncc build --source-map",
    "act-run-job": "act -P ubuntu-latest=nektos/act-environments-ubuntu:18.04 -W ./__tests__/workflows/ -s GITHUB_TOKEN=$GITHUB_TOKEN -j",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achieve-3000/yeoman-generator-runner.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Achieve 3000. <developers@achieve3000.com>",
  "license": "MIT",
  "homepage": "https://github.com/achieve-3000/yeoman-generator-runner#readme",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@actions/glob": "^0.1.2",
    "yeoman-environment": "^2.0.0"
  },
  "devDependencies": {
    "@octokit/plugin-rest-endpoint-methods": "^5.3.1",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.23",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^15.6.1",
    "@types/yeoman-environment": "^2.10.3",
    "@typescript-eslint/parser": "^4.26.0",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.27.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "is-svg": ">=4.2.2",
    "jest-circus": "^27.0.4",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.2"
  }
}
